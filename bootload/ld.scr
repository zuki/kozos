OUTPUT_FORMAT("elf32-h8300")
OUTPUT_ARCH(h8300h)
ENTRY("_start")

SECTIONS
{
    . = 0x0;    /* メモリの先頭番地から配置 */

    .vectors : {
        vector.o(.data)
    }

    .text : {
        *(.text)
    }

    .rodata : {
        *(.strings)
        *(.rodata)
        *(.rodata.*)
        . = ALIGN(4);/* http://kozos.jp/books/makeos/index.html#binutils-new */
    }

    .data : {
        *(.data)
    }

    .bss : {
        *(.bss)
        *(COMMON)
    }
}
